#Prints list of dZ

B=open("Bfile","r")
Z=[]
L=[]
dZ=[]
for i in B:
    l=B.readline()
    r=l[25:30]
    Z.append(int(r))  
for i in Z:
    L.append(int(i))
for i, li in enumerate(L):
    for j, ji in enumerate(L):
        if j==i+3:
            r=ji-li
            dZ.append(int(r))
print(dZ)
B.close()
