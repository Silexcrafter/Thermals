#Prints list of dZ

B=open("Bfile","r")
Z=[]
L=[]
dZ=[]
for i in B:
    l=B.readline()
    r=l[25:30]
    Z.append(int(r))  
for i in Z:
    L.append(int(i))
for i, li in enumerate(L):
    for j, ji in enumerate(L):
        if j==i+3:
            r=ji-li
            dZ.append(int(r))
print(dZ)
B.close()

# If the value in M is positive, Prints the corresponding line of B
B=open("Bfile","r")
M=[11,2,-3,-4,56]
LB=[]
for i in range (5):
    l=B.readline().strip("\n")
    LB.append(l)
    I=M[i]
    if I>0:
        print(LB[i])
B.close()


