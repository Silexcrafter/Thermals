#Should work better...
B=open("Bfile","r")
C=open("Cfile","w+")
Z=[]
L=[]
dZ=[]
for i in B:
    l=B.readline()
    r=l[25:30]
    Z.append(int(r))
for i in Z:
    L.append(int(i))
for i, li in enumerate(L):
    for j, ji in enumerate(L):
        if j==i+3:
            r=int(ji-li)
            dZ.append(r)
#this works
B=open("Bfile","r")
C=open("Cfile","w+")
LB=[]
for i,vi in enumerate(dZ):   #incrÃ©mentation,valeur
    l=B.readline().strip("\n")
    LB.append(l)
    if vi>0:
        C.write(LB[i])
        C.write("\n")
B.close()
C.close()
