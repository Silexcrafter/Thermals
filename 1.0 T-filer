#This is pure gold. Creates T-file containing only the Blines of the thermals
Imput= input("Enter file name :")
F=open(Imput,"r")
T=open("T-%s" %f ,"w+")
B= open("Bfile", 'w+')
for i in F:
    l=F.readline()
    r=l[0:1]    
    if "B" in r:
        B.write(l)
F.close()
B.close()
B=open("Bfile","r")
Z=[]
dZ=[]
LB=[]
for i,li in enumerate(B):
    LB.append((li).strip("\n"))
for i in LB:
    r=i[25:30]
    Z.append(int(r))
for i, li in enumerate(Z):
    for j, ji in enumerate(Z):
        if j==i+3:
            r=int(ji-li)
            dZ.append(r)
for i,vi in enumerate(dZ):
    if vi>0:
        T.write(LB[i])
        T.write("\n")
B.close()
T.close()
